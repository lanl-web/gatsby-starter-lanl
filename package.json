{
  "name": "@lawebteam/gatsby-starter-lanl",
  "version": "1.0.0",
  "description": "Gatsby starter for lanl sites",
  "scripts": {
    "dev": "gatsby develop",
    "gatsby:build": "gatsby build",
    "create": "node --experimental-specifier-resolution=node util/create-component.mjs",
    "build": "npm run test && npm run clean && gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clean": "jest --clear-cache",
    "lint": "prettier-eslint $PWD/'**/*.{js,jsx}'",
    "lint:fix": "prettier-eslint --write $PWD/'**/*.{js,jsx}'",
    "commit": "npm run lint:fix && git add . && git cz",
    "release": "standard-version",
    "release:min": "npm run release -- --release-as minor",
    "release:push": "git push --follow-tags origin master && git checkout develop && git rebase master && git push origin develop",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lawebteam/gatsby-starter-lanl.git"
  },
  "author": "LANL Web Team",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/lawebteam/gatsby-starter-lanl//issues"
  },
  "homepage": "https://github.com/lawebteam/gatsby-starter-lanl/#readme",
  "dependencies": {
    "@lawebteam/lanl-components": "^1.0.3",
    "gatsby": "^2.32.0",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^3.1.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@openfonts/source-sans-pro_latin": "^1.44.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^10.0.0",
    "colorette": "^1.2.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.5",
    "standard-version": "^9.1.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged & npm run test & npm run build",
      "commit-msg": "commitlint -e --verbose $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx}": [
      "prettier-eslint --write $PWD/'**/*.{js,jsx}'"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
